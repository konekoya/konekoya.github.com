{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "plugins": [],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "globals": {
  },
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "comma-dangle": [2, "never"],
    "indent": [2, 2],
    "object-curly-spacing": [2, "always"],
    "array-bracket-spacing": [2, "always"],
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "func-names": 0,
    "no-else-return": 0,
    "no-console": 1,
    "no-throw-literal": 0,
    "id-length": 0,
    "no-unused-expressions": 0,
    "no-param-reassign": 0,
    "max-len": [1, 300]
  }
}
